#include <stdio.h>

int uniq(int n, int a[]);

int main(void) {
  int n;
  int a[10000];
  scanf("%d", &n);
  int i;
  for(i = 0; i < n; i++){
    scanf("%d", a + i);
  }
  printf("%d\n", uniq(n, a));
  return 0;
}

int uniq(int n, int a[]) {
  int i, j;
  int ret = 1;
  for(i = 0; i < n-1; i++) {
    for(j = i+1; j < n-1; j++) {
      if(a[i] == a[j]) {
        ret = 0;
      }
    }
  }
  return ret;
}
